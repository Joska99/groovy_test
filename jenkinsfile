node('agent1') {
    stage('test') {
        sh 'ls -lah'
        sh 'env'
    }
    stage('Multy-branch') {
        if (env.BRANCH_NAME == 'main') {
            stage('name') {
                echo 'main trigger'
                echo "$env.BRANCH_NAME"
            }
        } else {
            echo 'other branch'
            echo "$env.BRANCH_NAME"
        }
    }
}

