node('agent1') {
    stage('test') {
        sh 'ls -lah'
        sh 'env'
    }
    stage('Multy-branch') {
        def branchName = env.GIT_BRANCH ?: 'Unknown'
        if (env.GIT_BRANCH == 'main') {
            echo 'main trigger-11'
            echo "$env.GIT_BRANCH"
        } else {
            echo 'other branch'
            echo "$env.GIT_BRANCH"
        }
    }
}

