node('agent1') {
    stage('test-1') {
        sh 'ls -lah'
        sh "echo ${env.BRANCH_NAME}"
    }
    stage('Multy-branch') {
        if (env.BRANCH_NAME == 'main') {
            echo 'main trigger-11'
            sh "echo $BRANCH_NAME"
        } else {
            echo 'other branch'
            sh "echo $BRANCH_NAME"
        }
    }
}

