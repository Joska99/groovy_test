environment {
    BRANCH_NAME = env.BRANCH_NAME
}

node('agent1') {
    stage('test-1') {
        sh 'ls -lah'
        sh 'env'
        sh "echo ${BRANCH_NAME}"
    }
    stage('Multy-branch') {
        if (env.BRANCH_NAME == 'main') {
            echo 'main trigger-11'
        } else {
            echo 'other branch'
        }
    }
}

